// Test 4: Function declarations with parameters

fn add(a: int, b: int) -> int {
    return a + b
}

fn multiply(x: int, y: int) -> int {
    var result = x * y
    return result
}

fn main() -> int {
    var x: int = 5
    var y: int = 3
    var sum: int = add(x, y)
    var product: int = multiply(x, y)
    
    return sum + product
}