let MAX_ITERATIONS = 1000
var counter = 0

fn calculate(x: int, y: int) -> int {
    var temp = x * 2 + y * 3
    var result = 0
    
    if temp > 100 {
        var bonus = 50
        result = temp + bonus
    } else {
        var penalty = 10
        result = temp - penalty
    }
    
    return result
}

fn fibonacci(n: int) -> int {
    if n <= 1 {
        return n
    }
    
    var a = 0
    var b = 1
    var i = 2
    
    loop i: 2...n {
        var temp = a + b
        a = b
        b = temp
    }
    
    return b
}

fn factorial(n: int) -> int {
    if n <= 1 {
        return 1
    }
    
    var result = 1
    var i = 2
    
    loop i: 2...n {
        result = result * i
    }
    
    return result
}

fn main() {
    var x = 15
    var y = 25
    var calc_result = calculate(x, y)
    
    var fib_10 = fibonacci(10)
    var fact_5 = factorial(5)
    
    var total = calc_result + fib_10 + fact_5
    
    return total
}